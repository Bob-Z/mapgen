import * from "managed_mats.material"

material mapgen_red
{
   technique
   {
      pass
      {
         lighting on

         ambient 0.3 0.1 0.1 1
         diffuse 0.8 0.05 0.05 1
         emissive 0 0 0 1
      }
   }
}

